{
    "name": "abcreche/print",
    "description": "Generate Print Ready Document",
    "keywords": [
        "laravel",
        "print",
        "document",
        "printer"
    ],
    "license": "MIT",
    "homepage": "https://github.com/abcreche/print",
    "authors": [
        {
            "name": "Cl√©ment Rigo",
            "email": "clement@abcreche.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "require-dev": {
        "phpunit/phpunit": "^8.2",
        "orchestra/testbench": "^4.0",
        "mockery/mockery": "^1.0"
    },
    "autoload": {
        "psr-4": {
            "ABCreche\\Print\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "ABCreche\\Print\\Test\\": "tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "ABCreche\\Print\\PrintServiceProvider"
            ]
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit"
    }
}
